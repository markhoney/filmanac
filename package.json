		"colors": "^1.4.0",
		"cross-env": "^7.0.2",
		"csv-parse": "^4.12.0",
		"depcheck": "^1.2.0",
		"fanart.tv": "^2.1.0",
		"ffmpeg-extract-frames": "^2.0.2",
		"fluent-ffmpeg": "^2.1.2",
