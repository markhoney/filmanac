{
	"name": "inmovies.today",
	"description": "Today in Movies",
	"version": "0.1.0",
	"homepage": "https://inmovies.today",
	"author": "Mark Honeychurch <mark@honeychurch.org> (https://mark.honeychurch.org)",
	"repository": "https://github.com/markhoney/inmovies.today",
	"license": "ISC",
	"scripts": {
		"scrape": "node src/data/movieevents",
		"develop": "gridsome develop",
		"dev": "yarn develop",
		"bacon": "node node_modules/six-degrees-kevin-bacon-api/index",
		"dep": "depcheck",
		"build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 gridsome build",
		"predeploy": "yarn build",
		"serve": "serve dist",
		"deploy": "gh-pages -d dist",
		"explore": "gridsome explore",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore -f table .",
		"lint:fix": "npm run lint --fix",
		"share": "lt --port 8080 --subdomain aarond",
		"up": "yarn upgrade && yarn upgrade-interactive --latest && yarn extras",
		"napa": "napa",
		"extras:genres": "napa DjCisco/resource.images.moviegenreicons.transparent XBMC-Addons/resource.images.moviegenreicons.white XBMC-Addons/resource.images.moviegenreicons.filmstrip dproudmoore/resource.images.moviegenrefanart.metrocity Helly1206/resource.images.moviegenrefanart.xonfluence mistervee/resource.images.moviegenreicons.filmstrip-hd.colour mistervee/resource.images.moviegenreicons.filmstrip-hd.bw",
		"extras:studios": "napa XBMC-Addons/resource.images.studios.coloured XBMC-Addons/resource.images.studios.white braz96/resource.images.studios.squarehd",
		"extras:classifications": "napa wyrm65/resource.images.classificationicons.colour",
		"extras:countries": "napa djaiss/mapsicon braz96/resource.images.moviecountryicons.flags braz96/resource.images.moviecountryicons.maps",
		"extras:languages": "napa vdb86/resource.images.languageflags.completepack jeroenpardon/resource.images.languageflags.colour-flat",
		"extras": "yarn extras:genres && yarn extras:studios && yarn extras:classifications && yarn extras:countries && yarn extras:languages"
	},
	"devDependencies": {
		"@danestves/tailwindcss-darkmode": "^1.1.5",
