		"gridsome-plugin-feed": "^1.0.2",
		"gridsome-plugin-flexsearch": "^1.0.3",
		"gridsome-plugin-pwa": "^0.0.18",
		"gridsome-plugin-tailwindcss": "^3.0.1",
		"imdb-api": "^4.2.0",
		"napa": "^3.0.0",
